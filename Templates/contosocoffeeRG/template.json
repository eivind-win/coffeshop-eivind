{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerGroups_contosocoffeelondon_name": {
      "type": "String"
    },
    "containerGroups_contosocoffeenewyork_name": {
      "type": "String"
    },
    "registries_contosocoffeeregistry_name": {
      "type": "String"
    },
    "storageAccounts_contosocoffeestgact_name": {
      "type": "String"
    },
    "trafficManagerProfiles_contosocoffee_name": {
      "type": "String"
    }
